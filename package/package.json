{
  "name": "text-editor",
  "version": "1.0.0",
  "description": "this package is text editor based on draft.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "npm run exam & npm run watch",
    "build": "rollup -c && tsc-alias -p tsconfig.json ",
    "watch": "rollup -cw",
    "exam": "cd example && npm start",
    "clean": "rm -rf node_modules & rm -rf package-lock.json & rm -rf lib & rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "http://git.uinetworks.kr/IntelligentSolutionTeam/text-editor.git"
  },
  "keywords": [
    "text",
    "editor",
    "draft.js",
    "react.js"
  ],
  "author": "ohtaekwon",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/classnames": "^2.3.1",
    "@types/draft-js": "^0.11.18",
    "@types/fabric": "^5.3.7",
    "@types/react": "^18.3.1",
    "@types/react-color": "^3.0.12",
    "autoprefixer": "^10.4.19",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-prefixer": "^3.0.0",
    "rollup": "^4.17.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "draft-js": "^0.11.7",
    "react": "^18.3.1",
    "react-color": "^2.19.3"
  },
  "dependencies": {}
